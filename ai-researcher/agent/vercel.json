{
  "version": 2,
  "build": {
    "env": {
      "POETRY_VERSION": "1.7.1"
    }
  },
  "builds": [
    {
      "src": "ai_researcher/demo.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "ai_researcher/demo.py"
    }
  ]
}